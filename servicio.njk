---
pagination:
  data: serviciosFull
  size: 1
  addAllPagesToCollections: true
  alias: servicio
tags:
  - servicio
  - {{servicio.categoria | slug}}
permalink: "servicio/{{servicio.categoria | slug}}/{{servicio.unidad_ejecutora | slug}}/{{servicio.nombre | slug}}/index.html"
---

{% extends "base.njk" %}


{% block head %}

{% endblock %}

{% block content %}

  <!--CONTENIDO-->

  <div class="w-100 bg-white">
    <div class="container py-5">
      <div class="row">
        
        <div class="bg-white col-lg-7">
          <br>
          <div class="d-flex">
            <a  href="index.html" style="color: #03324e!important;text-decoration: underline!important;"> Inicio</a>
            <p>&nbsp / &nbsp</p>
            <p>{{servicio.categoria}}</p>
            <p>&nbsp / &nbsp</p>
            <a href="#" style="color: #03324e!important;text-decoration: underline!important;">{{servicio.unidad_ejecutora}}</a>
          </div>
          <br>

          <h1 class="break-word">{{servicio.nombre}}</h1>
          <small>

          
            Información proporcionada por: <br>
            {{servicio.unidad_ejecutora}}
          </small>
          <br>
          <small>
            Última actualización: <br>
            13/01/2021
          </small>
          <br>
          <br>
          <h4 class="text-left">Descripción</h4>
          <p>{{servicio.nombre}}</p>
          <br>
          <a target="_blank" href="{{servicio.enlace}}" class="btnAzulO centrado5">Ir al trámite en línea</a> <br>
        </div>

        <div class="bg-white col-lg-5">
          <div class="container">
            <div class="col-12" style="justify-content: center;">

              <h4>Puntea este servicio!</h4>

              <div class="containerR">
                <div class="postR">
                  <div class="textR">Gracias por el Comentario!</div>
                  <div class="edit">EDITAR</div>
                </div>
                <div class="star-widget">
                  <input type="radio" name="rate" id="rate-5">
                  <label for="rate-5" class="fas fa-star"></label>
                  <input type="radio" name="rate" id="rate-4">
                  <label for="rate-4" class="fas fa-star"></label>
                  <input type="radio" name="rate" id="rate-3">
                  <label for="rate-3" class="fas fa-star"></label>
                  <input type="radio" name="rate" id="rate-2">
                  <label for="rate-2" class="fas fa-star"></label>
                  <input type="radio" name="rate" id="rate-1">
                  <label for="rate-1" class="fas fa-star"></label>
                  <form action="#">
                    <header></header>
                    <div class="textarea">
                      <textarea cols="30" placeholder="Describe la puntuación.."></textarea>
                    </div>
                    <div class="btn">
                      <button type="submit">Comentar</button>
                    </div>
                  </form>
                </div>
              </div>
                
            </div>
          </div>
        </div>

      </div>
    </div>
  </div>

  <!--PAGINAS IMPORTANTES-->
  <div class="w-100">
    <div class="container py-5">
      <div class="row">
        
        <div class="col-md-4 border">
          <br>
          <h4 class="text-left" style="color: #03324e">Publicado por Institucion</h4>  
          <h3>Consulta de tramite</h3>
          <p><strong>Tramite en linea</strong></p>

---
permalink: "instituciones/index.html"
---
{% extends "src/base.njk" %}

{% block head %}

    <title>Instituciones</title>

{% endblock %}

{% block content %}
        <section class="hero-3 position-relative align-items">
            <h2 class="mb-30 text-center text-white">Instituciones</h2>
        </section>
        <div class="page-header breadcrumb-wrap">
            <div class="container">
                <div class="breadcrumb">
                    <a href="/index.html" rel="nofollow"><i class="fi-rs-home mr-5"></i>Inicio</a>
                </div>
            </div>
        </div>
        <div class="page-content pt-50">
            <div class="container">
                <div class="row mb-40">
                    <div class="search-style-1">
                        <form action="#" class="m-auto">
                            <!--select class="select-active">
                                <option>All categories</option>
                                <option>Milks and Dairies</option>
                                <option>Wines & Alcohol</option>
                                <option>Clothing & Beauty</option>
                                <option>Pet Foods & Toy</option>
                                <option>Fast food</option>
                                <option>Baking material</option>
                                <option>Vegetables</option>
                                <option>Fresh Seafood</option>
                                <option>Noodles & Rice</option>
                                <option>Ice cream</option>
                            </select-->
                            <input id="searchField2" type="search" placeholder="Busca alguna Institución..." />
                            <ol id="searchResults2" class="search-container" style="display:none"/>
                        </form>
                    </div>
                </div>
                <div class="row vendor-grid">
                    <div id="lista_instituciones">
                        <ol>
                        {% for institucion in collections.instituciones_comp%}

                        <div class="col-lg-6 col-md-6 col-12 col-sm-6">
                            <div class="vendor-wrap style-2 mb-40">
                                <div class="product-badges product-badges-position product-badges-mrg ">
                                    <span class="hot">Institución</span>
                                </div>
                                <div class="vendor-img-action-wrap">
                                    <div class="vendor-img">
                                        <a href="{{institucion.url}}">
                                            <img class="default-img" src="/ELEMENTOS/ELEMENTOS MEJORAS/ICONOS/ICONOS-22.png" alt="" />
                                        </a>
                                    </div>
                                    <!--div class="mt-10">
                                        <span class="font-small total-product">380 products</span>
                                    </div-->
                                </div>
                                <div class="vendor-content-wrap w-100">
                                    <div class="mb-30 mt-30">
                                        <!--div class="product-category">
                                            <span class="text-muted">Since 2012</span>
                                        </div-->
                                        <div class="overflow-hidden" style="height:60px">
                                            <h4 class="mb-5"><a href="{{institucion.url}}">{{institucion.data.instituciones.name}}</a></h4>
                                        </div>
                                        <!--div class="product-rate-cover">
                                            <div class="product-rate d-inline-block">
                                                <div class="product-rating" style="width: 90%"></div>
                                            </div>
                                            <span class="font-small ml-5 text-muted"> (4.0)</span>
                                        </div-->
                                        <div class="vendor-info d-flex justify-content-between align-items-end mt-30">
                                            <ul class="contact-infor text-muted">
                                                <div class="overflow-hidden" style="height:45px">
                                                    <li><img src="assets/imgs/theme/icons/icon-location.svg" alt="" />{{institucion.data.instituciones.description}}</li>
                                                </div>
                                                <!--li><img src="assets/imgs/theme/icons/icon-contact.svg" alt="" /><strong>Call Us:</strong><span>(+91) - 540-025-124553</span></li-->
                                            </ul>
                                            <a href="{{institucion.url}}" class="btn btn-xs">Ver más <i class="fi-rs-arrow-small-right"></i></a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!--end vendor card-->

                        {% endfor %}
                        </ol>
                    </div>
                </div>
            </div>
        </div>
{% endblock %}
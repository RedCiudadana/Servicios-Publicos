---
pagination:
  data: servicios
  size: 1
  alias: servicio
  addAllPagesToCollections: true
permalink: "servicio/{{servicio.id}}/index.html"
tags:
  - servicio
---
{% extends "src/base.njk" %}


{% block head %}
    <title>Catálogo de Trámites | {{servicio.name}}</title>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css" integrity="sha512-9usAa10IRO0HhonpyAIVpjrylPvoDwiPUiKdWk5t3PyolY1cOd4DSE0Ga+ri4AuTroPR5aQvXU9xC6qOPnzFeg==" crossorigin="anonymous" referrerpolicy="no-referrer" />
    <!--LINK DE REFERENCIA PARA RATING https://www.youtube.com/watch?v=KdO9P8FnVN4&t=0s-->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>

{% endblock %}

{% block content %}

    <div class="page-header breadcrumb-wrap">
        <div class="container">
            <div class="breadcrumb">
                <a href="/index2.html" rel="nofollow"><i class="fi-rs-home mr-5"></i>Inicio</a>
                <span></span> {%for item in collections.categoriaesp2 %}{% if item.data.categoriaesp2.name == servicio.subcategory.name %} <a href="{{item.url}}">{{item.data.categoriaesp2.name}}</a> {%endif%}{%endfor%}
                <span></span> {{servicio.name}}
            </div>
        </div>
    </div>
    <div class="page-content mb-50">
        <div class="container">
            <div class="row">
                <div class="col-xl-11 col-lg-12 m-auto">
                    <div class="row">
                        <div class="col-lg-3 primary-sidebar sticky-sidebar pt-50">
                            <div class="widget-area">
                                <div class="sidebar-widget widget-category-2 mb-50">
                                    <h5 class="section-title style-1 mb-30">Especificaciones</h5>
                                    <ul>
                                        <li>
                                            <a style="pointer-events:none" href="shop-grid-right.html"> <img src="/ELEMENTOS/ICONOS/ICONOS-04.png" alt="" />{{servicio.institution.name}}</a>
                                        </li>
                                        <li>
                                            <a style="pointer-events:none" href="shop-grid-right.html"> <img src="/ELEMENTOS/ICONOS/ICONOS-05.png" alt="" />Resultado: {{servicio.typeOfDocumentObtainable}}</a>
                                        </li>
                                        <li>
                                            <a style="pointer-events:none" href="shop-grid-right.html"> <img src="/ELEMENTOS/ICONOS/ICONOS-06.png" alt="" />Costo: Q {{servicio.cost}} </a>
                                        </li>
                                        <li>
                                            <a style="pointer-events:none" href="shop-grid-right.html"> <img src="/ELEMENTOS/ICONOS/ICONOS-07.png" alt="" />Tiempo de Respuesta: {{servicio.timeResponse}}</a>
                                        </li>
                                        <li>
                                            <a href="/quejas.html"> <img src="/ELEMENTOS/ICONOS/ICONOS-09.png" alt="" />¿Tienes problemas con este trámite? Comunícate con nosotros</a>
                                        </li>
                                    </ul>
                                </div>
                                <!-- Product sidebar Widget -->
                                <div class="sidebar-widget widget-category-2 mb-50">
                                    <h5 class="section-title style-1 mb-30">Servicios Populares</h5>
                                    <ul>
                                        {% for servicio in collections.serviciosHighlighted | limit(3) %}
                                        {% if servicio.url !== page.url%}
                                        <li>
                                            <a href="{{servicio.url}}"> {{servicio.data.servicio.name}}</a>
                                        </li>
                                        {% endif %}
                                        {% endfor %}
                                    </ul>
                                </div>
                                <!--Tags-->
                                <div class="sidebar-widget widget-category-2 mb-50">
                                    <h5 class="section-title style-1 mb-30">Tags</h5>
                                    <ul>
                                        <li>
                                            <a style="pointer-events:none" href="shop-grid-right.html"> Trámite</a>
                                        </li>
                                    </ul>
                                </div>
                                <!--POSIBLES ANUNCIOS-->
                                <!--div class="banner-img wow fadeIn mb-50 animated d-lg-block d-none">
                                    <img src="/assets/imgs/banner/banner-11.png" alt="" />
                                    <div class="banner-text">
                                        <span>Oganic</span>
                                        <h4>
                                            Save 17% <br />
                                            on <span class="text-brand">Oganic</span><br />
                                            Juice
                                        </h4>
                                    </div>
                                </div-->
                            </div>
                        </div>
                        
                        <div class="col-lg-9">
                            <div class="single-page pt-50 pl-30">
                                <div class="single-header style-2">
                                    <div class="row">
                                        <div class="col-xl-10 col-lg-12 m-auto">
                                            <h6 class="mb-10"><a href="#">Trámite</a></h6>
                                            <h2 class="mb-10">{{servicio.name}}</h2>
                                        </div>
                                    </div>
                                </div>
                                <div class="single-content">
                                    <div class="row">
                                        <div class="col-xl-10 col-lg-12 m-auto">
                                            <p>{{servicio.description | rmj |safe}}</p>
                                            <h4>Normativa</h4>
                                            <p>{{servicio.normative | rmj |safe}}</p>
                                            <h4>Requerimientos</h4>
                                            <p>{{servicio.requirements | rmj |safe}}</p>
                                            <h4>Pasos</h4>
                                            <p>{{servicio.instructions | rmj |safe}}</p>
                                            <!--Comment form-->
                                            <div class="comment-form">
                                                <h3 class="mb-15">Dejanos tu Comentario</h3>
                                                <div class="row">
                                                    <div class="col-lg-9 col-md-12">
                                                        <form class="rating">
                                                            <label>
                                                                <input type="radio" name="stars" value="1" />
                                                                <span class="icon">★</span>
                                                            </label>
                                                            <label>
                                                                <input type="radio" name="stars" value="2" />
                                                                <span class="icon">★</span>
                                                                <span class="icon">★</span>
                                                            </label>
                                                            <label>
                                                                <input type="radio" name="stars" value="3" />
                                                                <span class="icon">★</span>
                                                                <span class="icon">★</span>
                                                                <span class="icon">★</span>   
                                                            </label>
                                                            <label>
                                                                <input type="radio" name="stars" value="4" />
                                                                <span class="icon">★</span>
                                                                <span class="icon">★</span>
                                                                <span class="icon">★</span>
                                                                <span class="icon">★</span>
                                                            </label>
                                                            <label>
                                                                <input type="radio" name="stars" value="5" />
                                                                <span class="icon">★</span>
                                                                <span class="icon">★</span>
                                                                <span class="icon">★</span>
                                                                <span class="icon">★</span>
                                                                <span class="icon">★</span>
                                                            </label>
                                                        </form>
                                                        <form class="form-contact comment_form mb-50" action="#" id="commentForm">
                                                            <div class="row">
                                                                <div class="col-12">
                                                                    <div class="form-group">
                                                                        <textarea class="form-control w-100" name="comment" id="comment" cols="30" rows="9" placeholder="Write Comment"></textarea>
                                                                    </div>
                                                                </div>
                                                                <div class="col-sm-6">
                                                                    <div class="form-group">
                                                                        <input class="form-control" name="name" id="name" type="text" placeholder="Name" />
                                                                    </div>
                                                                </div>
                                                                <div class="col-sm-6">
                                                                    <div class="form-group">
                                                                        <input class="form-control" name="email" id="email" type="email" placeholder="Email" />
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <div class="form-group">
                                                                <button type="submit" class="button button-contactForm">Post Comment</button>
                                                            </div>
                                                        </form>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        
                    </div>
                </div>
            </div>
        </div>
    </div>

{% endblock %}
